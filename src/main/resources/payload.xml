<Payload xmlns="http://javaops.ru"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://javaops.ru payload.xsd">
<!--    1.1 добавить проекты. Имеют название (например topjava, masterjava) и описание-->
    <Projects>
        <Project name="topjava">
            <description>Enterprise Java-разработчик (TopJava)</description>
<!--            1.2 добавить группы. Они имеют название и тип (REGISTERING/CURRENT/FINISHED) и принадлежат проекту.-->
            <Group name="topjava01" type="FINISHED"/>
            <Group name="topjava02" type="CURRENT"/>
            <Group name="topjava03" type="REGISTERING"/>
        </Project>
        <Project name="masterjava">
            <description>Многомодульный Maven, многопоточность, JavaEE</description>
            <Group name="masterjava01" type="CURRENT"/>
        </Project>
    </Projects>
    <Users>
<!--        1.3 сделать User.email аттрибутом.-->
<!--        1.4 реализовать принадлежность участников разным группам (Admin состоит в группах topjava07, topjava08, masterjava01)-->
        <User flag="active" city="kiv" email="active@ru.ru" userGroup="topjava01">
            <fullName>full Name</fullName>
        </User>
        <User flag="active" city="kiv" email="active1@r" userGroup="topjava03">
            <fullName>another User</fullName>
        </User>
        <User flag="superuser" city="spb" email="superuser@r.r" userGroup="topjava02 topjava03 masterjava01">
            <fullName>Axmin</fullName>
        </User>
        <User flag="deleted" city="spb" email="deleted@y.y">
            <fullName>Deleted</fullName>
        </User>
    </Users>
    <Cities>
        <City id="spb">Санкт-Петербург</City>
        <City id="kiv">Киев</City>
        <City id="mnsk">Минск</City>
    </Cities>
</Payload>